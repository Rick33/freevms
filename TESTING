tested feature, with 64/32, version, ods2/ext2

(Also test scripts in dcl/src/*.com and sys$system:*.com)

mount dqa2 (0_3_16_dev)

set default (0_3_16_dev)
show default (0_3_16_dev)
show time (0_3_16_dev)
show working_set (0_3_16_dev)
show logical sys$system (0_3_16_dev)
define mylogical test (0_3_16_dev)
write sys$output "test" (0_3_16_dev)
sysman + exit (0_3_16_dev)
install add sys$system:sda (0_3_16_dev)
dir (0_3_16_dev, ext2->ods2/ext2)
dir /file (0_3_16_dev, ext2->ods2/ext2)
dir /size (0_3_16_dev, ext2->ods2/ext2)
dir /date (0_3_16_dev, ext2->ods2/ext2)
dir /acl (0_3_16_dev, ext2->ods2/ext2)
type sys$system:install.com (0_3_16_dev, ext2->ext2)
create /dir dqa2:[000000]test.dir (0_3_16_dev, ext2->ods2)
copy sys$system:directory.exe dqa2:[test] (only 32 0_3_16_dev, ext2->ods2 no result, 64 crash)

show system (0_3_16_dev)
show memory (0_3_16_dev)
symbol="symbol" (only 32 0_3_16_dev, 64 crashing)
show symbol symbol (only 32 0_3_16_dev, 64 untested)